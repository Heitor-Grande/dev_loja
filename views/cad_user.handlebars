        <form action="/insert_user" method="post">
            <h3>NOME:</h3>
            <input type="text" name="senderName" id="senderName">
            <h3>CPF/CNPJ: </h3>
            <input type="text" name="senderCpfCnpj" id="cpfcnpj">
            <h3>TELEFONE: </h3>
            <input type="text" name="senderPhone" id="telefone">
            <h3>EMAIL:</h3>
            <input type="text" name="senderEmail" id="email">
            <h3>SENHA:</h3>
            <input type="password" name="pass" id="pass">
            <h3>CONFIRMAR SENHA:</h3>
            <input type="password" id="pass_confirm">
            <h2>-- ENDEREÃ‡O --</h2>
            <h3>CEP:</h3>
            <input type="text" name="shippingAddressPostalCode" id="cep">
